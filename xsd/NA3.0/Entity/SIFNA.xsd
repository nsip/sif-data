						</xs:complexType>
					</xs:element>
					<xs:element name="incidentId" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
