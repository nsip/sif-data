	<xs:complexType name="schoolCalendarItemType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>School Calendar Item</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A single event in a calendar. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSchoolCalendarItemType">
				<xs:sequence>
					<xs:element name="schoolCalendarRefId" type="gRefIdPointerType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The Id (UUID) of the school calendar of which this calendar date is part.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="schoolRefId" type="gRefIdPointerType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The Id (UUID) of the school for which this calendar information is being reported.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="schoolYear" type="xs:gYear" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>School year for which the information is applicable, expressed as the four-digit year in which the school year ends (e.g., "2007" for the 2006-07 school year).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="event" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId>000603</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3596</cedsURL>
							</xs:appinfo>
							<xs:documentation>The type of scheduled or unscheduled event that causes interruption in direct instruction.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="eventDayName" type="xs:normalizedString" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar/>
								<cedsId>001276</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=4242</cedsURL>
							</xs:appinfo>
							<xs:documentation>A name used for the day of the calendar event.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="date" type="xs:date">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>A specific school day that occurs within the school year.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateTypeCode" type="gCodedElementType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Code indicating the type of school day.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateNumber" type="xs:int" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Order in which the calendar date falls within the school calendar.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="studentAttendance" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="countsTowardAttendance" type="gYesNoUnknownType" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Designates whether this date should be counted toward student attendance.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="attendanceValue" type="xs:decimal" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Amount of the school day in which the student should be in attendance (Format is x.x; a student who should be in attendance a full day would be represented as 1.0). If CountsTowardAttendance equals Yes then StudentAttendanceValue must be greater than zero.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="teacherAttendance" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="countsTowardAttendance" type="gYesNoUnknownType" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Designates whether this date should be counted toward teacher attendance.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="attendanceValue" type="xs:decimal" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Amount of the school day in which the teacher should be in attendance (format is x.x; a teacher who should be in attendance a full day would be represented as 1.0).</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="administratorAttendance" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="countsTowardAttendance" type="gYesNoUnknownType" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Designates whether this date should be counted toward administrator attendance.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="attendanceValue" type="gYesNoUnknownType" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Amount of the school day in which the administrator should be in attendance (format is x.x; an administrator who should be in attendance a full day would be represented as 1.0).</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="bellScheduleDayList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="bellScheduleDay" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="bellScheduleRefId" type="gRefIdPointerType" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>The GUID of a bell schedule to be utilized for the calendar date.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="timetableDayIdentifier" type="xs:token" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>The unique identifier of a timetable day to be utilized for the calendar date (from the list of timetable days defined for the calendar in the referenced BellSchedule object).</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="schoolCalendarItem" type="schoolCalendarItemType"/>
	<xs:complexType name="schoolCalendarType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>School Calendar</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A school's calendar information. The Calendar Item object lists individual calendar events. A school may have more than one calendar (e.g., one calendar for kindergarten students, one for special education students, one for seniors, or one for each calendar track), and each may have different days in session, start and end dates, minutes per day, instructional minutes, etc.</xs:documentation>
		</xs:annotation>
