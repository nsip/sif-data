<?xml version="1.0" encoding="utf-8"?>
<!--version 3.00.13-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sifassociation.org/datamodel/us/3.0" targetNamespace="http://www.sifassociation.org/datamodel/us/3.0" elementFormDefault="qualified">
	<xs:include schemaLocation="SIFglobal.xsd"/>
	<!--Persons-->
	<xs:complexType name="contactPersonType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Contact Person</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>Demographic and other information for each person who might be referenced as a contact.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gContactPersonType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="contactPerson" type="contactPersonType"/>
	<xs:complexType name="personType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Person</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>An individual within an educational setting.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPersonType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="person" type="personType"/>
	<xs:complexType name="personLongitudinalType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Person Longitudinal Type</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>Used to connect an instances of a person at different education levels. For example connects a person instance to a student instance and connects the same person instance to a post secondary student instance.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPersonSubtypeType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="personLongitudinal" type="personLongitudinalType"/>
	<xs:complexType name="staffPersonType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Staff Person</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains all the personal information relating to a staff member, who might be a teacher or other employee of the school or district.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gStaffPersonType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="staffPerson" type="staffPersonType"/>
	<xs:complexType name="studentType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Student</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains all the personal information related to the student.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gStudentType">
				<xs:sequence>
					<xs:element name="disability" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="iDEAIndicator" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId>000151</cedsId>
											<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3151</cedsURL>
										</xs:appinfo>
										<xs:documentation>A person having intellectual disability; hearing impairment, including deafness; speech or language impairment; visual impairment, including blindness; serious emotional disturbance (hereafter referred to as emotional disturbance); orthopedic impairment;</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Status" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId>000577</cedsId>
											<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3569</cedsURL>
										</xs:appinfo>
										<xs:documentation>An indication of whether a person is classified as disabled under the American's with Disability Act (ADA).</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="section504Status" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId>000249</cedsId>
											<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3249</cedsURL>
										</xs:appinfo>
										<xs:documentation>Individuals with disabilities who are being provided with related aids and services under Section 504 of the Rehabilitation Act of 1973, as amended.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="primaryDisability" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId>000218</cedsId>
											<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3218</cedsURL>
										</xs:appinfo>
										<xs:documentation>The major or overriding disability condition that best describes a person's impairment.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="awaitingInitialIDEAEvaluation" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId>000031</cedsId>
											<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3031</cedsURL>
										</xs:appinfo>
										<xs:documentation>Awaiting initial evaluation for special education programs and related services under the Individuals with Disabilities Education Act (IDEA).</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="iDEAEnvironment" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId>000535</cedsId>
											<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3526</cedsURL>
										</xs:appinfo>
										<xs:documentation>The setting in which children ages 6 through 21, receive special education and related services.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="student" type="studentType"/>
	<!--Organizations-->
	<xs:complexType name="leaType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>LEA</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains information about a school district or other Local Educational Agency.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gLEAType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="lea" type="leaType"/>
	<xs:complexType name="schoolType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>School</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains information about the school.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSchoolType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="school" type="schoolType"/>
	<xs:complexType name="seaType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>SEA</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object is designed to facilitate the transfer of State Education Agency (SEA) directory information to the Department of Education (USED) through the Education Data Exchange Network (EDEN).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gEducationOrganizationType">
				<xs:sequence>
					<xs:element name="localId" type="gLocalIdType" minOccurs="1">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>M</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The locally-assigned identifier for this SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ncesId" type="gExternalIdType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The NCES-assigned identifier for this SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="seaName" type="xs:normalizedString" minOccurs="1">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>M</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The full legally accepted name of the SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="seaURL" type="xs:anyURI" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>URL for the SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ccssoContact" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Chief State School Officer contact information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="seaContactList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A list of contacts for the SEA.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="seaContact" type="gContactPersonType" minOccurs="1" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="phoneNumberList" type="gPhoneNumberListType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The SEA's phone number(s).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressList" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The SEA's address(es).</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="address" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="edenInfo" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="stateCode" type="gCodedElementType" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>The two-digit Federal Information Processing Standard (FIPS) Code for the State, District of Columbia, and the possessions and freely associated areas of the United States (e.g. Puerto Rico).</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="agencyIdNumber" type="xs:token" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>A number used in EDEN to uniquely uniquely identify state agencies.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="sea" type="seaType"/>
	<xs:complexType name="sectionType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Section</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A setting in which organized instruction of course content is provided to one or more students (including cross-age groupings) for a given period of time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSectionType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="section" type="sectionType"/>
	<!--Learning-->
	<xs:complexType name="courseType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Course</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object describes course information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gCourseType">
				<xs:sequence>
					<xs:element name="courseFrequency" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An indication of how often the course is offered, e.g., term, 1 year, 2 years, 3 years.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="course" type="courseType"/>
	<xs:complexType name="learningResourceType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Learning Resource</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains information related to learning resources that may be used in educational settings. These include textbooks, Internet content, educational software, videos and DVDs, supplemental print material, etc. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSIF_EntityType">
				<xs:sequence>
					<xs:element name="resourceName" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Name of learning resource.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="author" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Name of organization or company that created the resource.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contactList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="contact" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="name" type="xs:normalizedString" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Name of contact person.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="address" type="gAddressType" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Address of organization or company.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phoneNumber" type="gPhoneNumberType" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Phone number of organization or company.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="email" type="gEmailType" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>The email of the organization or company.</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="location" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="locationReference" type="xs:token" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>Reference to the resource, i.e. location in library or URL, community resource, outside resource supplier.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="referenceType" type="xs:token" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>A qualifying attribute for the Location payload. If ReferenceType is "URI", the payload contains a web address where the resource can be found.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="status" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Describes availability status of resource, e.g. "checked out."</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="description" type="xs:normalizedString" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Description of the resource, e.g., "This textbook is intended for grade 4 social studies students and addresses..."</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="yearGroupList" type="gYearGroupList">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>A list of grade levels</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="subjectAreaList" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>A list of subject areas.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="subjectArea" type="gSubjectAreaType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="mediaTypeList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="mediaType" type="xs:token" maxOccurs="unbounded">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>R</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>MIME type [MIME] based on IANA registration (see RFC2048) or 'non-digital'.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="useAgreement" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Describes terms of use for resource.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="agreementDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>A date that defines the date of agreement.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="approvalList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="approval" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="organization" type="xs:token" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Name of agency that approved use of resource.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="approvalDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Date resource was approved.</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="evaluationList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="evaluation" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="description" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Description of evaluation of resource.</xs:documentation>
