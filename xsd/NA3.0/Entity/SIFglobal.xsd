<?xml version="1.0" encoding="utf-8"?>
<!--version 3.00.13-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="SIFglobalCommonTypes.xsd"/>
	<!--The types in this file are SIF Global Entities.  They descend from the SIF Conceptual Model for Education. They serve as parent objects to more concrete entities and some are function as concrete objects as well. -->
	<xs:complexType name="gAcademicPlanType">
		<xs:annotation>
			<xs:documentation>A specific plan for student learning.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPlanType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gAddressType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>Code that defines the location of the address. Note: A subset of specific valid values for each instance in a data object.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gLocationType">
				<xs:sequence>
					<xs:element name="localId" type="gLocalIdType" minOccurs="0"/>
					<xs:element name="street" type="gStreetType"/>
					<xs:element name="city" type="xs:normalizedString"/>
					<xs:element name="subregion" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A second-order subdivision of the country, e.g. county, shire.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="stateProvince" type="gStateProvinceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A first-order national subdivision of the country, e.g. state, province.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="country" type="gCountryType" minOccurs="0"/>
					<xs:element name="postalCode" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A number that identifies each postal delivery area in a country used as a portion of an address</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gAreaType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A physical place that can be desribed by a polygon. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPlaceType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gAssociationType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>Represents functional associations among individuals, organizations, events, programs, etc. These associations include things such as student enrollment in a school, a contact person for an organization, or a sponsor for an event.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSIF_EntityType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gBellScheduleType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>Provides the bell schedule information for a school and school year, including timetable days, periods and bell schedules.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gEventCollectionType">
				<xs:sequence>
					<xs:element name="schoolYear" type="xs:gYear"/>
					<xs:element name="bellScheduleName" type="xs:token"/>
					<xs:element name="timetableDayList" type="gTimetableDayList"/>
					<xs:element name="timetablePeriodList" type="gTimetablePeriodList"/>
					<xs:element name="bellPeriodList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="bellPeriod" type="gBellPeriodType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="daysPerCycle" type="xs:int" minOccurs="0"/>
					<xs:element name="periodsPerDay" type="xs:int" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gClientType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>Client is an extension of Person.Someone who is served by an education institution.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPersonType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gContactPersonType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A person who might be referenced as a contact for some purpose.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gClientType">
				<xs:sequence>
					<xs:element name="publishIndicator" type="gYesNoUnknownType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Indicates if the contact information can be published in directories and other lists</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="employerType" type="gEmployerTypeType" minOccurs="0"/>
					<xs:element name="educationLevel" type="gEducationLevel" minOccurs="0"/>
					<xs:element name="positionTitle" type="xs:token" minOccurs="0"/>
					<xs:element name="role" type="xs:token" minOccurs="0"/>
					<xs:element name="addressRefId" type="gRefIdPointerType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gContactPersonAssociationType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>The relationship of a contact person to another person.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPersonAssociationType">
				<xs:sequence>
					<xs:element name="personRefId" type="gGenericRefIdPointerType"/>
					<xs:element name="contactPersonRefId" type="gRefIdPointerType"/>
					<xs:element name="relationship" type="gRelationshipType">
						<xs:annotation>
							<xs:documentation>The relationship between the person and the contact person, e.g., husband, wife.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contactFlagList" type="gContactFlagListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A list of association types such as: ParentLegalGuardian, PickupRights, LivesWith, AccessToRecords, EmergencyContact, HasCustody, DisciplinaryContact, PrimaryCareProvider.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contactSequence" type="xs:unsignedShort" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An integer indicating the order in which the person should be contacted in relation to other contacts or the priority in which this person should be contacted.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contactSequenceSource" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The source used to determine the contact sequence or priority, e.g., court order, transportation system, etc. </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gCourseType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object describes course information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gAcademicPlanType">
				<xs:sequence>
					<xs:element name="organizationRefId" type="gGenericRefIdPointerType"/>
					<xs:element name="yearOffered" type="xs:gYear"/>
					<xs:element name="courseCode" type="xs:token"/>
					<xs:element name="subjectAreaList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="subjectArea" type="gSubjectAreaType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="courseTitle" type="xs:token"/>
					<xs:element name="description" type="xs:normalizedString" minOccurs="0"/>
					<xs:element name="instructionalLevel" type="gInstructionalLevelType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gDiscreteEventType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A discrete event that occurs at a particular point in time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gEventType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gEducationGroupingType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>EducationGrouping is an extension of Organization.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gOrganizationType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gEducationOrganizationType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>EducationOrganization is an extension of Organization</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gOrganizationType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gEventCollectionType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A calendar.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPlanType">
				<xs:sequence>
					<xs:element name="organizationRefId" type="gGenericRefIdPointerType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The organization for which this calendar applies.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="startDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The first calendar date, whether or not instruction was provided. If both SchoolCalendar and SchoolCalendar Item objects are supported, StartDate must be the date of the first chronological instance of SchoolCalendar Item for the school's calendar.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="endDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The last calendar date, whether or not instruction was provided. EndDate must be the last chronological instance of SchoolCalendarItem for the school's calendar.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="duration" type="xs:duration" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This element can be used when startDate and/or endDate are not present. The duration of the event using the form of xs:duration.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gEventType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A temporal event.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSIF_EntityType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gIntervalEventType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>An event over time with begin and end points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gEventType">
				<xs:sequence>
					<xs:element name="startDate" type="xs:date" minOccurs="0"/>
					<xs:element name="endDate" type="xs:date" minOccurs="0"/>
					<xs:element name="duration" type="xs:duration" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The length of the term in days, months, etc. </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gK12CourseType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>K12Course is a school course and extends Course</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gCourseType">
				<xs:sequence>
					<xs:element name="termInfoRefId" type="gRefIdPointerType" minOccurs="0"/>
					<xs:element name="externalCourseCode" type="gExternalIdList" minOccurs="0"/>
					<xs:element name="courseCreditList" type="gCourseCreditListType" minOccurs="0"/>
					<xs:element name="graduationRequirement" type="gYesNoUnknownType" minOccurs="0"/>
					<xs:element name="department" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gLearnerType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gClientType">
				<xs:sequence>
					<xs:element name="otherIdList" type="gOtherIdListType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gLearningGoalType">
		<xs:complexContent>
			<xs:extension base="gPlanType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gLearningStandardItemType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains information related to curriculum standards statements "standards" or "benchmarks" or the like within the document. Each LearningStandardItem reflects an individual standard statement and may occur at several levels within a hierarchially structured document.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gLearningGoalType">
				<xs:sequence>
					<xs:element name="refURI" type="xs:anyURI" minOccurs="0"/>
					<xs:element name="shortName" type="xs:token" minOccurs="0"/>
					<xs:element name="itemVersion" type="xs:token" minOccurs="0"/>
					<xs:element name="description" type="xs:normalizedString" minOccurs="0"/>
					<xs:element name="organizationList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="organization" type="xs:normalizedString" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Name of the organization</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="standardHierarchyLevelNumber" type="xs:int"/>
					<xs:element name="standardHierarchyLevelDescription" type="xs:normalizedString"/>
					<xs:element name="predecessorItemList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="predecessorItem" type="gRefIdPointerType" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>RefIds of predecessor Learning Standard Items</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="statementCodeList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="statementCode" type="xs:token" maxOccurs="unbounded">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>R</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>An alphanumeric Id code as defined by the organization to identify the statement.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="statementList">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="statement" type="xs:string" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="yearGroupList" type="gYearGroupList" minOccurs="0"/>
					<xs:element name="subjectAreaList">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="subjectArea" type="gSubjectAreaType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="alternativeIdCodeList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="alternativeIdCode" type="xs:token" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="learningStandardDocumentRefId" type="gRefIdPointerType"/>
					<xs:element name="relatedLearningStandardItemList" type="gRelatedLearningStandardItemList" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gLearningStandardDocumentType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A curriculum standards document or the like published by a national, state, district, school site, professional association or other interested party. The LearningStandardDocument reflects an expectation of student work.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gLearningGoalType">
				<xs:sequence>
					<xs:element name="refURI" type="xs:anyURI" minOccurs="0"/>
					<xs:element name="title" type="xs:token"/>
					<xs:element name="description" type="xs:string" minOccurs="0"/>
					<xs:element name="documentVersion" type="xs:token" minOccurs="0"/>
					<xs:element name="organizationList">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="organization" type="xs:normalizedString" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Name of organization represented by the document, i.e., "National Council of Teachers of Mathematics (NCTM)," "ACARA"," NSW DET". </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="source" type="gLearningStandardsOrganizationType"/>
					<xs:element name="authorList" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="author" type="xs:token" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="organizationContactPoint" type="xs:string" minOccurs="0"/>
					<xs:element name="subjectAreaList">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="subjectArea" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="documentStatus" type="gDocumentPublishingStatusType"/>
					<xs:element name="documentDate" type="xs:date" minOccurs="0"/>
					<xs:element name="localAdoptionDate" type="xs:date" minOccurs="0"/>
					<xs:element name="localArchiveDate" type="xs:date" minOccurs="0"/>
					<xs:element name="endOfLifeDate" type="xs:date" minOccurs="0"/>
					<xs:element name="copyrightDate" type="xs:date" minOccurs="0"/>
					<xs:element name="copyrightHolder" type="xs:normalizedString" minOccurs="0"/>
					<xs:element name="yearGroupList" type="gYearGroupList" minOccurs="0"/>
					<xs:element name="learningStandardItemRefId" type="gRefIdPointerType"/>
					<xs:element name="relatedLearningStandardItemList" type="gRelatedLearningStandardItemList" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gLEAType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object contains information about a school district, region, or other Local, State or Jurisdiction Educational Authority.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gEducationOrganizationType">
				<xs:sequence>
					<xs:element name="localId" type="gLocalIdType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Local agency assigned Id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalIdList" type="gExternalIdList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A list of external agency assigned Ids. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="leaName" type="xs:string"/>
					<xs:element name="leaURL" type="xs:anyURI" minOccurs="0"/>
					<xs:element name="educationAgencyType" type="gLeaTypeType" minOccurs="0"/>
					<xs:element name="leaContactList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A list of contacts for the LEA</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="leaContact" type="gContactPersonType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="phoneNumberList" type="gPhoneNumberListType" minOccurs="0"/>
					<xs:element name="emailList" type="gEmailListType" minOccurs="0"/>
					<xs:element name="yearGroupList" type="gYearGroupList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A list of year groups, aka grade levels (North America).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="operationalStatus" type="gOperationalStatusType" minOccurs="0"/>
					<xs:element name="electoralDistrict" type="gElectoralDistrictType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gLocationType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>A single point in space or place.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gPlaceType">
				<xs:sequence>
					<xs:element name="gridLocation" type="gGridLocationType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="gNamedOrganizationAssociationType">
		<xs:annotation>
			<xs:appinfo>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gOrganizationAssociationType">
				<xs:sequence>
					<xs:element name="memberRefId" type="gGenericRefIdPointerType"/>
					<xs:element name="organizationRefId" type="gGenericRefIdPointerType"/>
					<xs:element name="membershipType" type="xs:token">
