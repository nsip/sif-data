													<xs:appinfo>
														<elementName/>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>The value of the score.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="passFailIndicator" type="gYesNoUnknownType" minOccurs="0">
												<xs:annotation>
