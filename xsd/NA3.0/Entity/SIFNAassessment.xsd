												<xs:annotation>
													<xs:appinfo>
														<elementName/>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>Lowest possible score value.</xs:documentation>
