							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="assessmentSession" type="assessmentSessionType"/>
	<xs:complexType name="assessmentResponseSetType">
		<xs:complexContent>
			<xs:extension base="gSIF_EntityType">
				<xs:sequence>
					<xs:element name="assessmentAdministrationRefId" type="gRefIdPointerType" minOccurs="0">
