						</xs:annotation>
					</xs:element>
					<xs:element name="FinishDateTime" type="xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName/>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Date and time testing ends.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="asessmentRefIdList">
						<xs:complexType>
							<xs:sequence>
