<?xml version="1.0" encoding="utf-8"?>
<!--version 3.00.13-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="SIFglobalBaseTypes.xsd"/>
	<!--Enumerated Types, i.e., coded elements that are enumerated inline in the XSD instead of having an external codeset  -->
	<xs:simpleType name="gAlertMessageTypeType">
		<xs:annotation>
			<xs:documentation>This attribute specifies what type of alert message this is.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gAssociationTypeType">
		<xs:annotation>
			<xs:documentation>The association between a person and the organization they are affiliated with.This is meant to enumerate the relevant subclasses of Person defined by organizational affiliatione.g.  Student,  Staff, PersonContact. The enumeration may extend beyond those subclasses.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<!--Simple Common Types-->
	<xs:simpleType name="gAddressRoleType">
		<xs:annotation>
			<xs:documentation>A facet of Address</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gApplicationFunctionType">
		<xs:annotation>
			<xs:documentation>The primary function of an application, e.g., student information or library management.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gAuthenticationTypeType">
		<xs:annotation>
			<xs:documentation>Defines how authentication is performed and exchanged.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gBirthdateVerificationType">
		<xs:annotation>
			<xs:documentation>Means by which the persons birth date was validated.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gCitizenshipStatusType">
		<xs:annotation>
			<xs:documentation>The persons citizenship status.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gContactFlagType">
		<xs:annotation>
			<xs:documentation>Contains a list of the contact persons role. Provide an instance for all characteristics that apply. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gCreditTypeEarnedType">
		<xs:annotation>
			<xs:documentation>Number of credits earned.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="gDocumentPublishingStatusType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gDwellingArrangementType">
		<xs:annotation>
			<xs:documentation>Setting/environment in which the person resides.</xs:documentation>
		</xs:annotation>
		<xs:list itemType="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gEducationLevel">
		<xs:annotation>
			<xs:documentation>A code representing the highest level of education completed by a person.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gElectoralDistrictType">
		<xs:annotation>
			<xs:documentation>The electoral district.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gElectronicIdTypeType">
		<xs:annotation>
			<xs:documentation>The type of the identifier that is read by electronic equipment. The types include Barcode, Magstripe, PIN, RFID, etc.</xs:documentation>
		</xs:annotation>
		<xs:list itemType="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gEmailTypeType">
		<xs:annotation>
			<xs:documentation>This attribute specifies the type of e-mail address.</xs:documentation>
		</xs:annotation>
		<xs:list itemType="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gEmployerTypeType">
		<xs:annotation>
			<xs:documentation>Code that defines the employer type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gEncryptionType">
		<xs:annotation>
			<xs:documentation>A named encryption algorithm.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gEntryTypeType">
		<xs:annotation>
			<xs:documentation>Container elements for EntryType information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gExitStatusType">
		<xs:annotation>
			<xs:documentation>Closure status for this enrollment.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gExitTypeType">
		<xs:annotation>
			<xs:documentation>The type of exit for this enrollment</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gFullTimeStatusType">
		<xs:annotation>
			<xs:documentation>An indication of whether the student is enrolled full time or part time.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gGenderOfEntryType">
		<xs:annotation>
			<xs:documentation>Gender of school population.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gGraduationDate">
		<xs:annotation>
			<xs:documentation>Date student officially graduated from secondary education.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="gPartialDateType"/>
	</xs:simpleType>
	<xs:simpleType name="gInstructionalLevelType">
		<xs:annotation>
			<xs:documentation>An indication of the general nature and difficulty of instruction provided.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gJobFunctionType">
		<xs:annotation>
			<xs:documentation>The purpose of the activities as related to students.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gLanguageProficiencyType">
		<xs:annotation>
			<xs:documentation>The persons proficiency in the primary language of education in the school. In the existing SIF locales, the language is English.</xs:documentation>
		</xs:annotation>
		<xs:list itemType="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gLanguageTypeType">
		<xs:annotation>
			<xs:documentation>An indication of the function and context in which an individual uses a language to communicate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gLeaTypeType">
		<xs:annotation>
			<xs:documentation>Type of LEA.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gLearningStandardsItemRelationshipType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="gLearningStandardsOrganizationType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="gListedStatusType">
		<xs:annotation>
			<xs:documentation>Indicates whether or not the phone number is available to the public.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="gYesNoUnknownType"/>
	</xs:simpleType>
	<xs:simpleType name="gMaritalStatusType">
		<xs:annotation>
			<xs:documentation>The persons marital status.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gMedicalAlertMessageSeverityType">
		<xs:annotation>
			<xs:documentation>The level of severity of this medical alert.</xs:documentation>
		</xs:annotation>
		<xs:list itemType="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gMembershipTypeType">
		<xs:annotation>
			<xs:documentation>The type of association between the party and the organization. May encompass existing SIF associations such as K12StudentEnrollment.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gNameRoleType">
		<xs:annotation>
			<xs:documentation>Code that specifies what type of name this is. The type has a different codeset for different Locales, which is not represented at the global tier by a fixed codeset.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gOperationalStatusType">
		<xs:annotation>
			<xs:documentation>Operational condition of an institution.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gOtherIdTypeType">
		<xs:annotation>
			<xs:documentation>Other Id type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gPhoneNumberTypeType">
		<xs:annotation>
			<xs:documentation>Code that specifies what type of phone number this is. Note: A subset of valid values may be specified in data objects.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gPromotionStatusType">
		<xs:annotation>
			<xs:documentation>This value should be set if this enrollment instance is closed for an end-of-year closeout or a mid-year promotion or demotion. A value other than N/A should be specified when the enrollment instance represents an end-of-year closeout or mid-year promotion/demotion.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gRecordClosureReasonType">
		<xs:annotation>
			<xs:documentation>The reason why this enrollment was closed.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gRelationshipType">
		<xs:annotation>
			<xs:documentation>The relationship of the contact person to the person, e.g., father, mother, parent, guardian.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gResidencyStatusType">
		<xs:annotation>
			<xs:documentation>Location of an individuals legal residence relative to (within or outside) the boundaries of the school for this enrollment.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gRoleTypeType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gSchoolSectorType">
		<xs:annotation>
			<xs:documentation>School sector of school, e.g. Public/Private.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gSchoolFocusType">
		<xs:annotation>
			<xs:documentation>The type of educational institution as classified by its focus.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gSchoolTypeType">
		<xs:annotation>
			<xs:documentation>An indication of the level of the educational institution.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gSessionTypeType">
		<xs:annotation>
			<xs:documentation>Code that specifies the session type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gSubjectAreaType">
		<xs:annotation>
			<xs:documentation>This represents the subject or content area this curriculum item addresses.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="gTeachingAssignmentType">
		<xs:annotation>
			<xs:documentation>A field taught by the individual.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<!-- Complex Common Types-->
	<xs:complexType name="gAlertMessageType">
		<xs:annotation>
			<xs:documentation>An alert message associatied with the student. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="message"/>
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="messageType"/>
	</xs:complexType>
	<xs:complexType name="gBaseNameType">
		<xs:sequence>
			<xs:element name="prefix" type="xs:token" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<sifChar>O</sifChar>
						<cedsId/>
						<cedsURL/>
					</xs:appinfo>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="familyName" type="xs:normalizedString">
				<xs:annotation>
					<xs:appinfo>
						<sifChar>O</sifChar>
						<cedsId/>
						<cedsURL/>
					</xs:appinfo>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="givenName" type="xs:normalizedString">
				<xs:annotation>
					<xs:appinfo>
						<sifChar>O</sifChar>
						<cedsId/>
						<cedsURL/>
					</xs:appinfo>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="middleName" type="xs:normalizedString" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<sifChar>O</sifChar>
