	</xs:complexType>
	<xs:complexType name="gContactFlagListType">
		<xs:sequence>
			<xs:element name="contactFlag" type="gContactFlagType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<sifChar>O</sifChar>
						<cedsId/>
						<cedsURL/>
					</xs:appinfo>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
